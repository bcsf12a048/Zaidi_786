@{
    ViewBag.Title = "Client_list_cmpy";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}




<div class="page-title">
    <i class="icon-custom-left"></i>

    <h3>Ny Order &gt;Kundlista &gt; Företag</h3>
    <br />
</div>


<div class="col-lg-12">
    <ul id="myTab" class="nav nav-tabs">
        <li class=""><a id="b1" href="/Admin/Client_list_pvt">Privat</a></li>
        <li class="active"><a id="b2" href="/Admin/Client_list_cmpy">Företag</a></li>
    </ul>
    <div id="" class="container-fluid " style="background-color:white">
        <div class="col-sm-12">

            <!-- 1st r -->

            <div class="row" id="frm_pad" style="padding-top:12px">
                <div class="col-sm-4 form-group">
                    <input class="form-control input-lg focus" id="aliii" name="" placeholder="Sök" type="text">
                </div>
                <div class="col-sm-4 form-group">

                </div>
                <div class="col-sm-4 form-group">
                    <button id="search" class="btn btn-success btn-rounded pull-right" type="button" onclick="Search_client_nw_cmpy()">Sok</button>
                    <script>

                        function Search_client_nw_cmpy() {

                            var Cate = $("#aliii").val().toString();


                            $.getJSON("/Admin/SearchResult_new_client_cmpy",

                                {

                                    Cate: Cate
                                },

                                function (data) {
                                  //  alert(data.length);
                                    //console.log(data);
                                    if (data != null) {

                                        var str = " ";
                                        var str1 = " ";
                                        for (var i = 0; i < data.length; i++) {
                                            str += "<tr>"
                                            str += "<td id='r" + i + "'>" + data[i].OrganisationNumber + "</td>";
                                            str += "<td id='r" + i + "'>" + data[i].CompanyNumber + "</td>";
                                            str += "<td id='r" + i + "'>" + data[i].Kundnummer + "</td>";
                                            str += "<td id='r" + i + "'>" + data[i].ContactPerson + "</td>";
                                            str += "<td id='r" + i + "'>" + data[i].companyname + "</td>";
                                            str += "<td id='r" + i + "'>" + data[i].PhoneContactPerson + "</td>";
                                            str += "<td id='r" + i + "'>" + data[i].EmailContactPerson + "</td>";
                                            str += "<td id='r" + i + "'>" + data[i].CompanyPhoneNumber + "</td>";
                                            str += "<td id='r" + i + "'>" + data[i].CompanyFaxNumber + "</td>";
                                            str += "<td id='r" + i + "'>" + data[i].CompanyEmail + "</td>";
                                            str += "<td id='r" + i + "'>" + data[i].StreetAddress + "</td>";
                                            str += "<td id='r" + i + "'>" + data[i].Postcode + "</td>";
                                            str += "<td id='r" + i + "'>" + data[i].City + "</td>";
                                            str += "<td id='r" + i + "'>" + data[i].County + "</td>";
                                            str += "<td id='r" + i + "'>" + data[i].Id + "</td>";
                                            str += "<td id='r" + i + "'>";
                                            str += "<div class='dropdown'>";
                                            str += "<button class='btn btn-primary dropdown-toggle' type='button' data-toggle='dropdown'>Action<span class='caret'></span></button>";
                                            str += "<ul class='dropdown-menu'>";
                                            str += "<li><a href='/Admin/Client_list_cmpy_update/" + data[i].Id + "'>Redigera</a></li>";
                                            str += "<li><a href='/Admin/Client_list_cmpy_delete/" + data[i].Id + "'>Radera</a></li>";
                                            str += "</ul>";
                                            str += "</div>"
                                            str += "</td>";
                                            str += "</tr>";
                                        }
                                    }
                                    else if (data == false) {
                                        alert("Sorrry no record");
                                    }
                                    $('#client_cmpy').html(str);
                                });

                        };
                    </script>

                </div>



            </div>
        </div>
        <div class="container-fluid  " style="background-color: white; overflow-y: auto; overflow-x: auto; height: 500px;">

            <table class="table table-striped ">
                <caption>&nbsp;</caption>
                <thead>
                    <tr>
                        
                        <th>Organisations&nbsp;Nummer</th>
                        <th>Företags&nbsp;Nummer</th>
                        <th>Kundnumer</th>
                        <th>Kontakt&nbsp;Person</th>
                        <th>Företags&nbsp;Namn</th>
                        <th>Telefon&nbsp;Kontakt&nbsp;Person</th>
                        <th>E-post&nbsp;Kontakt&nbsp;Person</th>
                        <th>Växel &nbsp;Telefon&nbsp;</th>
                        <th>Växel &nbsp;Fax&nbsp;Nummer</th>
                        <th>E-post&nbsp;</th>
                        <th>Adress&nbsp;</th>
                        <th>Postkod</th>
                        <th>Ort</th>
                        <th>Län</th>
                        <th>Id</th>
                        <th>Åtgärd</th>

                    </tr>
                </thead>
                <tbody id="client_cmpy">
                   @foreach (var x in Model)
    {
        <tr>

            <td>@x.OrganisationNumber</td>
            <td>@x.CompanyNumber</td>
            <td>@x.Kundnummer</td>
            <td>@x.ContactPerson</td>
            <td>@x.companyname</td>
            <td>@x.PhoneContactPerson</td>
            <td>@x.EmailContactPerson</td>
            <td>@x.CompanyPhoneNumber</td>
            <td>@x.CompanyFaxNumber</td>
            <td>@x.CompanyEmail</td>
            <td>@x.StreetAddress</td>
            <td>@x.Postcode</td>
            <td>@x.City</td>
            <td>@x.County</td>
            <td>@x.Id</td>
            <td>
                <div class="dropdown">
                    <button class="btn btn-primary dropdown-toggle" type="button" data-toggle="dropdown">Action<span class="caret"></span></button>
                    <ul class="dropdown-menu">
                        <li><a href="/Admin/Client_list_cmpy_update/@x.Id">Redigera</a></li>
                        <li><a href="/Admin/Client_list_cmpy_delete/@x.Id">Radera</a></li>

                    </ul>
                </div>
            </td>

        </tr>

    }
                </tbody>
            </table>

        </div>
    </div>
</div>


