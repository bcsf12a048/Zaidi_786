@{
    ViewBag.Title = "Invoice_my_pvt_paid";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<div class="page-title">
    <i class="icon-custom-left"></i>

    <h3>Faktura &gt;Min Faktura  &gt; Privat</h3>
    <br />
</div>

<div class="col-lg-12">
    <ul id="myTab" class="nav nav-tabs">
        <li class="active"><a id="b1" href="/Admin/Invoice_my_pvt_all">Privat</a></li>
        <li class=""><a id="b2" href="/Admin/Invoice_my_cmpy">Företag</a></li>
    </ul>

    <div class="container-fluid  " style="background-color: white; overflow-y: auto; overflow-x: auto; height: 500px;">
       

        <ul id="myTab" class="nav nav-tabs" style="padding-top:12px">
            <li class=""><a id="b1" href="/Admin/Invoice_my_pvt_all">Allt</a></li>
            <li class="active"><a id="b2" href="/Admin/Invoice_my_pvt_paid">Betald</a></li>
            <li class=""><a id="b2" href="/Admin/Invoice_my_pvt_unpaid">Obetald</a></li>
        </ul>

        <table class="table table-striped">
            <caption>&nbsp;</caption>
            <thead>
                <tr>
                    <th>Order&nbsp;Nummer</th>
                    <th>Faktura&nbsp;Nummer</th>
                    <th>OCR</th>
                    <th>Faktura&nbsp;Datum</th>
                    <th>Förfallo Datum</th>
                    <th>Vår &nbsp;Referens</th>
                    <th>Dröjsmålsränta&nbsp;</th>
                    <th>påminnelseavgift&nbsp;</th>
                    <th>Status</th>
                    <th>AnvandarNamn</th>
                    <th>Åtgärd</th>

                </tr>
            </thead>
            <tbody>
                @foreach (var x in Model)
                {
                    <tr>

                        <td>@x.Order_no</td>
                        <td>@x.Invoice_no</td>
                        <td>@x.OCR</td>
                        <td>@x.InvoiceDate</td>
                        <td>@x.Exp_Date</td>
                        <td>@x.OurReference</td>
                        <td>@x.IntrestRate</td>
                        <td>@x.RemainderFee</td>
                        <td>@x.status</td>
                        <td>@x.username</td>
                        <td>
                            <div class="dropdown">
                                <button class="btn btn-primary dropdown-toggle" type="button" data-toggle="dropdown">Åtgärd<span class="caret"></span></button>
                                <ul class="dropdown-menu">
                                    <li><a href="/Admin/Invoice_my_pvt_paid_update/@x.Invoice_no">Redigera</a></li>
                                    <li><a href="/Admin/Invoice_my_pvt_paid_delete/@x.Invoice_no">Ta bort</a></li>
                                </ul>
                            </div>
                        </td>

                    </tr>

                }
            </tbody>
        </table>

    </div>
</div>
