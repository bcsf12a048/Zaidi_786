@model CarService.Models.client_nwod_pvt
@{
    ViewBag.Title = "Client_list_pvt_update";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}


<div class="page-title">
    <i class="icon-custom-left"></i>

    <h3>Kunder &gt;Lägg till ny kund &gt; Update &gt; Privat</h3>
    <br />
</div>

<div class="col-lg-12">
    <!-- Form Start -->
    <form action="/Admin/Client_list_pvt_update_update/@Model.Id" method="post">
        <div id="" class="container-fluid " style="background-color:white">
            <div class="col-sm-12">

                <!-- 1st r -->


                <div class="row" id="frm_pad" style="padding-top:12px">
                    <div class="col-sm-4 form-group">
                        <input class="form-control input-lg focus" id="" value="@Model.Licenseplate" name="Licenseplate" placeholder="Registrerings Nummer" type="text">
                        <span id="result"></span>

                    </div>

                    <div class="col-sm-4 form-group">
                        <input class="form-control input-lg focus" id="" value="@Model.ChassiNumber" name="ChassiNumber" placeholder="Chassi Nummer" type="text">
                    </div>

                    
                </div>

                <div id="new">

                </div>
                <!-- 2nd r -->

                <div class="row" id="frm_pad">
                    <div class="col-sm-4 form-group">
                        <input class="form-control input-lg focus" id="" value="@Model.FirstName" name="FirstName" placeholder="Förnamn" type="text">
                    </div>

                    <div class="col-sm-4 form-group">
                        <input class="form-control input-lg focus" id="" value="@Model.LastName" name="LastName" placeholder="Efternamn" type="text">
                    </div>

                </div>

                <!-- 3rd r -->

                <div class="row" id="frm_pad">
                    <div class="col-sm-4 form-group">
                        <input class="form-control input-lg focus" id="" value="@Model.PhoneNumber" name="PhoneNumber" placeholder="Telefon" type="text">
                    </div>

                    <div class="col-sm-4 form-group">
                        <input class="form-control input-lg focus" id="" value="@Model.Mobile" name="Mobile" placeholder="Mobil" type="number">
                    </div>

                    <div class="col-sm-4 form-group">
                        <input class="form-control input-lg focus" id="" value="@Model.Email" name="Email" placeholder="E-post" type="email">
                    </div>

                </div>

                <!-- 4th r -->


                <div class="row" id="frm_pad">
                    <div class="col-sm-4 form-group">
                        <input class="form-control input-lg focus" id="" value="@Model.Street_Address" name="Street_Address" placeholder="Adress" type="text">
                    </div>

                    <div class="col-sm-4 form-group">
                        <input class="form-control input-lg focus" id="" value="@Model.City" name="City" placeholder="Ort" type="text">
                    </div>

                    <div class="col-sm-4 form-group" style="padding-bottom:12px;">
                        <input class="form-control input-lg focus" id="" value="@Model.Postcode" name="Postcode" placeholder="Postkod" type="text">
                    </div>

                </div>

                <!-- 5th r -->

                <div class="row" id="frm_pad" style="padding-bottom:12px;">
                    <div class="col-sm-4 form-group">
                        <input class="form-control input-lg focus" id="" value="@Model.County" name="County" placeholder="Län" type="text">
                    </div>


                </div>


            </div>
        </div>
        <div class="row" id="frm_pad" style="padding-top:15px;">
            <div class="col-sm-12 form-group">
                <button class="btn btn-success btn-rounded pull-right" type="submit"> Spara</button>
            </div>

        </div>
    </form>

</div>

