@model IEnumerable<CarService.Models.companyservice>
@{
    ViewBag.Title = "Service_list_cmpy";
    Layout = "~/Views/Shared/_LayoutClientCompany.cshtml";
}


<div class="">

    <h3>Mina Tjänster &gt;Tjänster &gt; Företag</h3>
    <br />
</div>


<div class="col-lg-12">
    
    <div id="" class="container-fluid " style="background-color:white">
        <div class="col-sm-12">

            <!-- 1st r -->

            <div class="row" id="frm_pad" style="padding-top:12px">

                <div class="form-group col-sm-6 ">
                    <select name="" class="form-control " id="">
                        <option value="">Service Kategori</option>
                    @foreach (var x in Model)
{
    <option value="@x.Service_add_pakg_Company.ChooseCategory">@x.Service_add_pakg_Company.ChooseCategory</option>
}
                    </select>




                </div>

                <div class="form-group col-sm-6 ">
                    <select name="CompanyName" class="form-control " id="" required>
                        <option>Företags Namn</option>
                        @foreach (var x in Model)
                        {
                            <option value="@x.Service_add_pakg_Company.CompanyName">@x.Service_add_pakg_Company.CompanyName</option>
                        }
                    </select>
                </div>



            </div>
        </div>
        <div class="container-fluid  " style="background-color:white ;overflow-y:auto ;overflow-x:auto;">

            <table class="table table-striped">
                <caption>&nbsp;</caption>
                <thead>
                    <tr>
                        <th>Order&nbsp;Nummer</th>
                        <th>Service&nbsp;Kategori</th>
                      
                        <th>Tjänst&nbsp;</th>
                        <th>Pris&nbsp;EX&nbsp;Moms</th>
                        <th>Pris &nbsp;Inc&nbsp;Moms</th>
                        <th>Företags&nbsp;Namn</th>
                        <td>Reg Nr</td>
                        <td>Användarnamn</td>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var z in Model)
                    {
                        <tr>
                            <td>@z.NwOd_company.order_no</td>
                            <td>@z.Service_add_pakg_Company.ChooseCategory</td>

                            <td>@z.Service_add_pakg_Company.ServiceName</td>
                            <td>@z.Service_add_pakg_Company.PriceExVAT</td>
                            <td>@z.Service_add_pakg_Company.PriceIncVAT</td>
                            <td>@z.Service_add_pakg_Company.CompanyName</td>
                            <td>@z.NwOd_company.LicensePlate</td>
                            <td>@z.NwOd_company.username</td>

                        </tr>

                    }
                </tbody>
            </table>

        </div>
    </div>
</div>