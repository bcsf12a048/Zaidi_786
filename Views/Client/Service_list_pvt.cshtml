@model IEnumerable<CarService.Models.privateservice>

@{
    ViewBag.Title = "Service_list_pvt";
    Layout = "~/Views/Shared/_LayoutClientPrivate.cshtml";
}


<div class="">
    

    <h3>Mina Tjänster &gt;Tjänster &gt; Privat</h3>
    <br />
</div>


<div class="col-lg-12">
    
    <div id="" class="container-fluid " style="background-color:white">
        <div class="col-sm-12">

            <!-- 1st r -->

            <div class="row" id="frm_pad" style="padding-top:12px">

                <div class="form-group col-sm-6 ">
                    <select name="" class="form-control " id="aali">
                        <option value="">Service Kategori</option>
                      @foreach(var z in Model)
                      {
                          <option value="@z.Service_add_pakg_Pvt.ChooseCategory">@z.Service_add_pakg_Pvt.ChooseCategory</option>
                          
                      }
                    </select>



                </div>
                <div class="col-sm-4 form-group">
                    <button id="search" class="btn btn-success btn-rounded pull-right" type="button" onclick="Search_service_list_pvt_client()">Sok</button>
                    <script>

                        function Search_service_list_pvt_client() {
                            
                            var Cate = $("#aali").val().toString();

                            
                            $.getJSON("/Client/SearchResult_service_list_pvt_client",

                                {

                                    Cate: Cate
                                },

                                function (data) {
                                     alert(data.length);
                                   console.log(data);
                                    if (data != null) {

                                        var str = " ";
                                        var str1 = " ";
                                        for (var i = 0; i < data.length; i++) {
                                            str += "<tr>"
                                            str += "<td id='r" + i + "'>" + data[i].order_no + "</td>";
                                            str += "<td id='r" + i + "'>" + data[i].z[0].ChooseCategory + "</td>";
                                            str += "<td id='r" + i + "'>" + data[i].z[0].ServiceName + "</td>";
                                            str += "<td id='r" + i + "'>" + data[i].z[0].PriceExVAT + "</td>";
                                            str += "<td id='r" + i + "'>" + data[i].z[0].PriceIncVAT + "</td>";
                                            str += "<td id='r" + i + "'>" + data[i].username + "</td>";
                                            str += "<td id='r" + i + "'>" + data[i].LicensePlate + "</td>";
                                            str += "</tr>";
                                        }
                                    }
                                    else if (data == false) {
                                        alert("Sorrry no record");
                                    }
                                    $('#service_pvt_client').html(str);
                                });

                        };
                    </script>


                </div>
        </div>
        <div class="container-fluid  " style="background-color:white ;overflow-y:auto ;overflow-x:auto;">

            <table class="table table-striped ">
                <caption>&nbsp;</caption>
                <thead>
                    <tr>
                        <th>Order&nbsp;Nummer</th>
                        <th>Service&nbsp;Kategori</th>
                        <th>Tjänst&nbsp;</th>
                        <th>Pris&nbsp;EX&nbsp;Moms</th>
                        <th>Pris &nbsp;Inc&nbsp;moms</th>
                        <th>Användar&nbsp;Namn</th>
                        <th>Reg&nbsp;Nr</th>
                        

                    </tr>
                </thead>
                <tbody id="service_pvt_client">
                    @foreach (var z in Model)
                    {
                        <tr>
                            <td>@z.NwOd_private.order_no</td>
                            <td>@z.Service_add_pakg_Pvt.ChooseCategory</td>

                            <td>@z.Service_add_pakg_Pvt.ServiceName</td>
                            <td>@z.Service_add_pakg_Pvt.PriceExVAT</td>
                            <td>@z.Service_add_pakg_Pvt.PriceIncVAT</td>
                            <td>@z.NwOd_private.username</td>
                            <td>@z.NwOd_private.LicensePlate</td>

                        </tr>

                    }
                </tbody>
            </table>

        </div>
    </div>
</div>