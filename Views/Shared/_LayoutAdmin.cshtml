    <script>



        function creatediv() {
            // debugger;
            var value = $('#btn2').val();
            $.getJSON("/Admin/CheckValue", function (x) {

                //if (x >0) {

                //    for (var i = x; i>=1; i--) {
                //        $("B" + i).remove();

                //       // v=0;
                //    }
                if (x > 0) {
                    $("#new").empty();
                    for (var i = 1; i <= value; i++) {
                        mNewObj = document.createElement('div');
                        mNewObj.id = "B" + i;
                        mNewObj.innerHTML = "<div class='row' id='frm_pad'><div class='col-sm-4 form-group'><input class='form-control input-lg focus' id='' name='b" + i + "' placeholder='Registreringsskylt' type='text'></div><div class='col-sm-4 form-group'><input class='form-control input-lg focus' id='' name='c" + i + "' placeholder='chassinummer' type='text'></div></div>";
                        mNewObj.style.visibility = "show";

                        document.getElementById("new").appendChild(mNewObj);
                        $.getJSON("/Admin/updateValue?value=" + value)
                    };

                }
                else if (x == 0) {


                    for (var i = 1; i <= value; i++) {
                        mNewObj = document.createElement('div');
                        mNewObj.id = "B" + i;
                        mNewObj.innerHTML = "<div class='row' id='frm_pad'><div class='col-sm-4 form-group'><input class='form-control input-lg focus' id='' name='b" + i + "' placeholder='Registreringsskylt' type='text'></div><div class='col-sm-4 form-group'><input class='form-control input-lg focus' id='' name='c" + i + "' placeholder='chassinummer' type='text'></div></div>";
                        mNewObj.style.visibility = "show";

                        document.getElementById("new").appendChild(mNewObj);
                        $.getJSON("/Admin/updateValue?value=" + value)
                    };
                };

            });

        };


    </script>

