        <div id="signupModal" class="modal fade" role="dialog">
            <div class="modal-dialog" style="padding-top:20px;">
                <div class="modal-content" style="background-color:rgba(245, 245, 245, 0.34); color: black;">
                    <button id="closeSignup" style="padding-right:10px;" type="button" class="close" data-dissmiss="modal">&times;</button>
                    <h4 class="modal-title" style="font-size:xx-large;margin-left:10px">Registrera Form</h4>
                    <div class="modal-body">
                        <!--SignUp Form-->
                        <p style="text-align:left; margin-left:10px">Det är gratis och SignUp kommer att bli.</p>

                        <form action="/Client/SignUp" method="post">
                            <div class="form-group "  >
                          

                                <input class="form-control input-lg focus" type="text" name="username" id="signup" placeholder="AnvandarNamn" value="" style="color:black " />


                                <input class="form-control input-lg focus" id="signup" name="pin" value="" type="password" placeholder="Stift" style="color:black">
                                    
                                <input class="form-control input-lg focus" id="signup" name="firstname" value="" type="text" placeholder="För Namn" style="color:black">
                                
                                <input id="Name" name="lastname" class="form-control input-lg focus" placeholder="Efter Namn" value="" type="text" style="color:black">
                                <br /><br/>
                                    <label for="Role"><b>Användartyp</b></label>
                                    <input type="radio" style="position:relative" name="type" value="Company" checked><b>Företag</b>
                                    <input type="radio" style="position:relative" name="type" value="private"><b>Privat</b>
                                <br/><br/>
                                <input value="Registrera Dig" id="signup" class="btn btn-default " type="submit">
                            </div>

                        </form>
                    </div>
                </div>
            </div>
        </div>




        <!-- BEGIN MAIN CONTENT -->
        <div class="dashboard col-lg-12 col-md-12 col-sm-12 col-xs-12 " style="padding-left:15px;padding-right:15px;">
            @RenderBody()
